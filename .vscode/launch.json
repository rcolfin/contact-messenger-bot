{
    "configurations": [
        {
            "name": "Python Debugger: Python File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "env": {
                "EMAIL__AUTH__USER": "EMAIL__AUTH__USER",
                "EMAIL__AUTH__PASSWORD": "EMAIL__AUTH__PASSWORD"
            }
        },
        {
            "name": "Python Module: Python File",
            "type": "debugpy",
            "request": "launch",
            "module": "contact_messenger_bot.api",
            "cwd": "${workspaceFolder}/contact-messenger-bot-api",
            "args": [
                "list-contacts"
            ],
            "env": {
                "EMAIL__AUTH__USER": "EMAIL__AUTH__USER",
                "EMAIL__AUTH__PASSWORD": "EMAIL__AUTH__PASSWORD"
            }
        }
    ]
}
